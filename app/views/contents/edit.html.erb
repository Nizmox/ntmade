<h2>Your Content</h2>

<h4><%= @content.name %></h4>

<%= form_for @content, :class => "form-horizontal", :html => {:role => 'form'}  do |f| %>
<div class="form-group">
  <%= f.text_area :body, :class => "form-control", :rows => 10 %>
</div>
  <%= f.submit :class => "btn btn-default" %>
<% end %><br>

<%= link_to 'Back', setup_path %>